{
    "name": "CelebA",
    "n_gpu": 0,
    "arch": {
        "type": "HourglassNet",
        "args": {}
    },
    "dataset": {
        "type": "CelebAPrunedAligned_MAFLVal",
        "args": {
            "root": "/scratch/shared/nfs1/jdt/celeba"
        }
    },
    "warper": {
        "type": "Warper",
        "args": {

        }
    },
    "optimizer": {
        "type": "Adam",
        "args": {
            "lr": 0.001,
            "weight_decay": 0,
            "amsgrad": true
        }
    },
    "loss": "dense_correlation_loss",
    "metrics": [
        "my_metric"
    ],
    "lr_scheduler": {
        "type": "StepLR",
        "args": {
            "step_size": 50,
            "gamma": 0.1
        }
    },
    "trainer": {
        "epochs": 100,
        "save_dir": "saved/",
        "save_period": 1,
        "verbosity": 2,
        "monitor": "min val_loss",
        "early_stop": 10,
        "tensorboardX": true,
        "log_dir": "saved/runs"
    }
}
