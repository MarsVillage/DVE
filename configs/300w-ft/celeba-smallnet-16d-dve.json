{
    "finetune_from": "data/saved/models/celeba-smallnet-16d-dve/2019-08-02_06-20-13/checkpoint-epoch100.pth",
    "n_gpu": 1,
    "batch_size": 32,
    "arch": {
        "type": "SmallNet",
        "args": {
            "num_output_channels": 16
        }
    },
    "dataset": {
        "type": "ThreeHundredW",
        "args": {
            "root": "data/300w/300w",
            "imwidth": 100,
            "crop": 15,
            "use_keypoints": false,
            "do_augmentations": true
        }
    },
    "warper": {
        "type": "Warper",
        "args": {}
    },
    "optimizer": {
        "type": "Adam",
        "args": {
            "lr": 0.001,
            "weight_decay": 0,
            "amsgrad": true
        }
    },
    "loss": "dense_correlation_loss_dve",
    "metrics": [],
    "visualizations": [],
    "lr_scheduler": {
        "type": "MultiStepLR",
        "args": {
            "milestones": [
                30,
                40
            ],
            "gamma": 0.1
        }
    },
    "trainer": {
        "epochs": 50,
        "save_dir": "saved/",
        "save_period": 25,
        "verbosity": 2,
        "monitor": "off",
        "early_stop": 50,
        "tensorboardX": true,
        "log_dir": "saved/"
    }
}