{
    "name": "celeba_3d_keypoints",
    "n_gpu": 1,
    "batch_size": 16,
    "arch": {
        "type": "HourglassNet",
        "args": {
            "num_output_channels": 3
        }
    },
    "dataset": {
        "type": "CelebAPrunedAligned_MAFLVal",
        "args": {
            "root": "data/celeba",
            "imwidth": 170,
            "crop": 21
        }
    },
    "keypoint_regressor": {
        "type": "IntermediateKeypointPredictor",
        "args": {
            "num_annotated_points": 5,
            "num_intermediate_points": 10
        }
    },
    "warper": {
        "type": "WarperSingle",
        "args": {
        }
    },
    "warp_val": false,
    "optimizer": {
        "type": "Adam",
        "args": {
            "lr": 0.001,
            "weight_decay": 0,
            "amsgrad": true
        }
    },
    "loss": "regression_loss",
    "metrics": [

    ],
    "visualizations": [
        "keypoints_intermediate"
    ],
    "lr_scheduler": {
        "type": "StepLR",
        "args": {
            "step_size": 50,
            "gamma": 0.1
        }
    },
    "trainer": {
        "epochs": 100,
        "save_dir": "saved/",
        "save_period": 1,
        "verbosity": 2,
        "monitor": "min val_loss",
        "early_stop": 10,
        "tensorboardX": true,
        "log_dir": "saved/"
    }
}
