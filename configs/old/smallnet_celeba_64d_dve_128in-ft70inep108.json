{
    "name": "smallnet_celeba_64d_dve_128in-ft70inep108",
    "n_gpu": 4,
    "batch_size": 64,
    "bias_lr": 0.00001,
    "finetune_from": "saved/smallnet_celeba_64d_dve_checkpoint-epoch108.pth",
    "arch": {
        "type": "SmallNet",
        "args": {
            "num_output_channels": 64
        }
    },
    "dataset": {
        "type": "CelebAPrunedAligned_MAFLVal",
        "args": {
            "root": "data/celeba",
            "imwidth": 180,
            "crop": 26
        }
    },
    "warper": {
        "type": "Warper",
        "args": {
        }
    },
    "optimizer": {
        "type": "Adam",
        "args": {
            "lr": 0.0001,
            "weight_decay": 0,
            "amsgrad": true
        }
    },
    "loss": "dense_correlation_loss_dve",
    "loss_args": {"normalize_vectors": false},
    "metrics": [

    ],
    "visualizations": [
        "sphere_colormap",
        "sphere_rand_proj_colormap"
    ],
    "lr_scheduler": {
        "type": "StepLR",
        "args": {
            "step_size": 100,
            "gamma": 0.1
        }
    },
    "trainer": {
        "epochs": 100,
        "save_dir": "saved/",
        "save_period": 1,
        "verbosity": 2,
        "monitor": "off",
        "early_stop": 10,
        "tensorboardX": true,
        "log_dir": "saved/"
    }
}
