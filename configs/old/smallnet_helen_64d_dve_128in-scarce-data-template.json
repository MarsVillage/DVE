{
    "name": "smallnet_helen_64d_dve_128in-ep17-restrict_to-1",
    "n_gpu": 1,
    "finetune_from": "aws-saved/smallnet_celeba_64d_dve_128in_checkpoint-epoch57.pth",
    "batch_size": 32,
    "arch": {
        "type": "SmallNet",
        "args": {
            "num_output_channels": 64
        }
    },
    "dataset": {
        "type": "Helen",
        "args": {
            "root": "data/SmithCVPR2013_dataset_resized",
            "imwidth": 128,
            "crop": 0,
            "downsample_labels": false,
            "do_augmentations": false,
            "break_preproc": false,
            "rand_in": false,
            "restrict_to": 5,
            "restrict_seed": 0
        }
    },
    "segmentation_head": {
        "type": "SegmentationHead",
        "args": {
            "num_classes": 11,
            "freeze_base": true
        }
    },
    "warp_val": false,
    "optimizer": {
        "type": "Adam",
        "args": {
            "lr": 0.01,
            "weight_decay": 0.0005,
            "amsgrad": true
        }
    },
    "loss": "segmentation_loss",
    "loss_args": {},
    "metrics": [
    ],
    "visualizations": [
    ],
    "lr_scheduler": {
        "type": "MultiStepLR",
        "args": {
            "milestones": [100,250,350],
            "gamma": 0.1
        }
    },
    "trainer": {
        "epochs": 500,
        "save_dir": "saved/",
        "save_period": 1,
        "verbosity": 2,
        "monitor": "min val_loss",
        "early_stop": 10,
        "tensorboardX": true,
        "log_dir": "saved/",
        "log_miou": true
    }
}
