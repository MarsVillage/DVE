{
    "name": "smallnet_chimps_64d_dve_70in",
    "n_gpu": 1,
    "batch_size": 32,
    "bias_lr": 0.00001,
    "arch": {
        "type": "SmallNet",
        "args": {
            "num_output_channels": 64
        }
    },
    "dataset": {
        "type": "Chimps",
        "args": {
            "root": "data/chimpanzee_faces/datasets_cropped_chimpanzee_faces/data_CZoo",
            "imwidth": 70,
            "crop": 0
        }
    },
    "warper": {
        "type": "Warper",
        "args": {
        }
    },
    "optimizer": {
        "type": "Adam",
        "args": {
            "lr": 0.001,
            "weight_decay": 0,
            "amsgrad": true
        }
    },
    "loss": "dense_correlation_loss_dve",
    "loss_args": {"normalize_vectors": false, "fold_corr": false},
    "metrics": [

    ],
    "visualizations": [
        "sphere_colormap",
        "sphere_scatter3d",
        "sphere_norm_scatter3d",
        "sphere_rand_proj_colormap"
    ],
    "lr_scheduler": {
        "type": "StepLR",
        "args": {
            "step_size": 100,
            "gamma": 0.1
        }
    },
    "trainer": {
        "epochs": 300,
        "save_dir": "saved/",
        "save_period": 1,
        "verbosity": 2,
        "monitor": "off",
        "early_stop": 10,
        "tensorboardX": true,
        "log_dir": "saved/"
    }
}
