{
    "name": "smallnet_mafl_64d_dve_128in_keypoints-ep57-v1",
    "collate_fn": "dict_flatten",
    "n_gpu": 1,
    "batch_size": 128,
    "keypoint_regressor_upsample": false,
    "arch": {
        "type": "DummyNet",
        "args": {
            "num_output_channels": 64
        }
    },
    "dataset": {
        "type": "CachedDataset",
        "args": {
            "root": "data/celeba",
            "cache_root": "saved/feat_caches/smallnet_celeba_64d_dve_128in_checkpoint-epoch57-hqTrue-crop26-imwidth180",
            "imwidth": 180,
            "use_hq_ims": true,
            "crop": 26
        }
    },
    "keypoint_regressor": {
        "type": "IntermediateKeypointPredictor",
        "args": {
            "num_annotated_points": 5,
            "num_intermediate_points": 50,
            "softargmax_mul": 100.0
        }
    },
    "warp_val": false,
    "optimizer": {
        "type": "Adam",
        "args": {
            "lr": 0.01,
            "weight_decay": 0.0005,
            "amsgrad": true
        }
    },
    "loss": "regression_loss",
    "loss_args": {"alpha": 10.0},
    "metrics": [
        "inter_ocular_error"
    ],
    "visualizations": [
        "keypoints_intermediate"
    ],
    "lr_scheduler": {
        "type": "MultiStepLR",
        "args": {
            "milestones": [100,250,350],
            "gamma": 0.1
        }
    },
    "trainer": {
        "epochs": 500,
        "save_dir": "saved/",
        "save_period": 1,
        "verbosity": 2,
        "monitor": "off",
        "early_stop": 10,
        "tensorboardX": true,
        "log_dir": "saved/"
    }
}
