{
    "name": "smallnet_mafl_64d_dve_128in_keypoints-ep45-v3",
    "n_gpu": 1,
    "finetune_from": "saved/smallnet_celeba_64d_dve_128in_checkpoint-epoch45.pth",
    "batch_size": 128,
    "arch": {
        "type": "SmallNet",
        "args": {
            "num_output_channels": 64
        }
    },
    "dataset": {
        "type": "MAFLAligned",
        "args": {
            "root": "data/celeba",
            "imwidth": 180,
            "crop": 26,
            "use_keypoints": true
        }
    },
    "keypoint_regressor": {
        "type": "IntermediateKeypointPredictor",
        "args": {
            "num_annotated_points": 5,
            "num_intermediate_points": 50,
            "softargmax_mul": 100.0
        }
    },
    "warp_val": false,
    "optimizer": {
        "type": "SGD",
        "args": {
            "lr": 0.01,
            "weight_decay": 0.0005,
            "momentum": 0.9
        }
    },
    "loss": "regression_loss",
    "loss_args": {"alpha": 30.0},
    "metrics": [
        "inter_ocular_error"
    ],
    "visualizations": [
        "keypoints_intermediate"
    ],
    "lr_scheduler": {
        "type": "MultiStepLR",
        "args": {
            "milestones": [50,200,300],
            "gamma": 0.1
        }
    },
    "trainer": {
        "epochs": 500,
        "save_dir": "saved/",
        "save_period": 1,
        "verbosity": 2,
        "monitor": "min val_loss",
        "early_stop": 10,
        "tensorboardX": true,
        "log_dir": "saved/"
    }
}
