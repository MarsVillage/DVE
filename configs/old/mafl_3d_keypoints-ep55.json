{
    "name": "mafl_3d_keypoints-ep55",
    "n_gpu": 1,
    "finetune_from": "saved/celeba_3d_nonormalize_checkpoint-epoch55.pth",
    "batch_size": 128,
    "arch": {
        "type": "HourglassNet",
        "args": {
            "num_output_channels": 3
        }
    },
    "dataset": {
        "type": "MAFLAligned",
        "args": {
            "root": "data/celeba",
            "imwidth": 170,
            "crop": 21,
            "use_keypoints": true
        }
    },
    "keypoint_regressor": {
        "type": "IntermediateKeypointPredictor",
        "args": {
            "num_annotated_points": 5,
            "num_intermediate_points": 50
        }
    },
    "warper": {
        "type": "WarperSingle",
        "args": {
        }
    },
    "warp_val": false,
    "optimizer": {
        "type": "Adam",
        "args": {
            "lr": 0.1,
            "weight_decay": 0,
            "amsgrad": true
        }
    },
    "loss": "regression_loss",
    "metrics": [
        "inter_ocular_error"
    ],
    "visualizations": [
        "keypoints_intermediate"
    ],
    "lr_scheduler": {
        "type": "MultiStepLR",
        "args": {
            "milestones": [5,20,40],
            "gamma": 0.1
        }
    },
    "trainer": {
        "epochs": 100,
        "save_dir": "saved/",
        "save_period": 1,
        "verbosity": 2,
        "monitor": "min val_loss",
        "early_stop": 10,
        "tensorboardX": true,
        "log_dir": "saved/"
    }
}
