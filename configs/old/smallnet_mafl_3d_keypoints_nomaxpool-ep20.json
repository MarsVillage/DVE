{
    "name": "smallnet_mafl_3d_nomaxpool_keypoints-smallwarp-ep20",
    "n_gpu": 1,
    "finetune_from": "saved/smallnet_celeba_3d_dve_nomaxpool/0319_233554/checkpoint-epoch20.pth",
    "batch_size": 128,
    "arch": {
        "type": "SmallNet",
        "args": {
            "num_output_channels": 3
        }
    },
    "dataset": {
        "type": "MAFLAligned",
        "args": {
            "root": "data/celeba",
            "imwidth": 100,
            "crop": 15,
            "use_keypoints": true
        }
    },
    "keypoint_regressor": {
        "type": "IntermediateKeypointPredictor",
        "args": {
            "num_annotated_points": 5,
            "num_intermediate_points": 50
        }
    },
    "warp_val": false,
    "optimizer": {
        "type": "Adam",
        "args": {
            "lr": 0.1,
            "weight_decay": 0,
            "amsgrad": true
        }
    },
    "loss": "regression_loss",
    "metrics": [
        "inter_ocular_error"
    ],
    "visualizations": [
        "keypoints_intermediate"
    ],
    "lr_scheduler": {
        "type": "MultiStepLR",
        "args": {
            "milestones": [10,30,50,70],
            "gamma": 0.1
        }
    },
    "trainer": {
        "epochs": 100,
        "save_dir": "saved/",
        "save_period": 1,
        "verbosity": 2,
        "monitor": "off",
        "early_stop": 10,
        "tensorboardX": true,
        "log_dir": "saved/"
    }
}
