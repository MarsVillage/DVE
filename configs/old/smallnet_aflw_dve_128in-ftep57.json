{
    "name": "smallnet_aflw_64d_dve_128in-ftep57",
    "n_gpu": 1,
    "batch_size": 16,
    "bias_lr": 0.00001,
    "finetune_from": "aws-saved/smallnet_celeba_64d_dve_128in_checkpoint-epoch57.pth",
    "arch": {
        "type": "SmallNet",
        "args": {
            "num_output_channels": 64
        }
    },
    "dataset": {
        "type": "AFLW",
        "args": {
            "root": "data/aflw/aflw_release-2",
            "imwidth": 128,
            "use_minival": true,
            "crop": 0
        }
    },
    "warper": {
        "type": "Warper",
        "args": {
        }
    },
    "optimizer": {
        "type": "Adam",
        "args": {
            "lr": 0.0001,
            "weight_decay": 0,
            "amsgrad": true
        }
    },
    "loss": "dense_correlation_loss_dve",
    "loss_args": {"normalize_vectors": false, "fold_corr": true},
    "metrics": [

    ],
    "visualizations": [
        "sphere_colormap",
        "sphere_rand_proj_colormap"
    ],
    "lr_scheduler": {
        "type": "StepLR",
        "args": {
            "step_size": 30,
            "gamma": 0.1
        }
    },
    "trainer": {
        "epochs": 50,
        "save_dir": "saved/",
        "save_period": 1,
        "verbosity": 2,
        "monitor": "off",
        "early_stop": 10,
        "tensorboardX": true,
        "log_dir": "saved/"
    }
}
