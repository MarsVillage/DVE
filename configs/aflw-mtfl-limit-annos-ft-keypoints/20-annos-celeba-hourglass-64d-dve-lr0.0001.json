{
    "n_gpu": 1,
    "finetune_from": "data/saved/models/aflw-mtfl-ft-celeba-hourglass-64d-dve/2019-08-11_16-40-28/checkpoint-epoch50.pth",
    "batch_size": 32,
    "cache_descriptors": true,
    "restrict_annos": 20,
    "keypoint_regressor_upsample": false,
    "arch": {
        "type": "HourglassNet",
        "args": {
            "num_output_channels": 64
        }
    },
    "dataset": {
        "type": "AFLW_MTFL",
        "args": {
            "root": "data/aflw-mtfl",
            "imwidth": 136,
            "crop": 20,
            "use_hq_ims": true,
            "use_keypoints": true,
            "do_augmentations": true
        }
    },
    "keypoint_regressor": {
        "type": "IntermediateKeypointPredictor",
        "args": {
            "num_annotated_points": 5,
            "num_intermediate_points": 50,
            "softargmax_mul": 100.0
        },
        "softmaxarg_mul": 100.0
    },
    "warper": {
        "type": "WarperSingle",
        "args": {}
    },
    "warp_val": false,
    "optimizer": {
        "type": "Adam",
        "args": {
            "lr": 0.0001,
            "weight_decay": 0.0005,
            "amsgrad": true
        }
    },
    "loss": "regression_loss",
    "loss_args": {
        "alpha": 10.0
    },
    "metrics": [
        "inter_ocular_error"
    ],
    "visualizations": [],
    "lr_scheduler": {
        "type": "MultiStepLR",
        "args": {
            "milestones": [
                60,
                80
            ],
            "gamma": 0.1
        }
    },
    "trainer": {
        "epochs": 100,
        "save_dir": "saved/",
        "save_period": 1,
        "verbosity": 2,
        "monitor": "off",
        "early_stop": 300,
        "tensorboardX": true,
        "log_dir": "saved/"
    }
}