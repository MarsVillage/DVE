{
    "name": "smallnet_aflw_64d_evc_128in_keypoints-ep57-v1",
    "n_gpu": 1,
    "finetune_from": "aws-saved/smallnet_celeba_64d_evc_128in_checkpoint-epoch57.pth",
    "batch_size": 64,
    "keypoint_regressor_upsample": true,
    "arch": {
        "type": "SmallNet",
        "args": {
            "num_output_channels": 64
        }
    },
    "dataset": {
        "type": "AFLW",
        "args": {
            "root": "data/aflw/aflw_release-2",
            "imwidth": 128,
            "crop": 0,
            "use_keypoints": true,
            "do_augmentations": false
        }
    },
    "keypoint_regressor": {
        "type": "IntermediateKeypointPredictor",
        "args": {
            "num_annotated_points": 5,
            "num_intermediate_points": 50,
            "softargmax_mul": 100.0
        }
    },
    "warp_val": false,
    "optimizer": {
        "type": "Adam",
        "args": {
            "lr": 0.01,
            "weight_decay": 0.0005,
            "amsgrad": true
        }
    },
    "loss": "regression_loss",
    "loss_args": {"alpha": 10.0},
    "metrics": [
        "inter_ocular_error"
    ],
    "visualizations": [
        "keypoints_intermediate"
    ],
    "lr_scheduler": {
        "type": "MultiStepLR",
        "args": {
            "milestones": [100,250,350],
            "gamma": 0.1
        }
    },
    "trainer": {
        "epochs": 500,
        "save_dir": "saved/",
        "save_period": 1,
        "verbosity": 2,
        "monitor": "min val_loss",
        "early_stop": 10,
        "tensorboardX": true,
        "log_dir": "saved/"
    }
}
