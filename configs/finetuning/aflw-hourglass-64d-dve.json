{
    "n_gpu": 1,
    "batch_size": 16,
    "arch": {
        "type": "HourglassNet",
        "args": {
            "num_output_channels": 64
        }
    },
    "dataset": {
        "type": "AFLW",
        "args": {
            "root": "data/aflw/aflw_release-2",
            "imwidth": 136,
            "crop": 20,
            "use_hq_ims": true,
            "use_keypoints": false,
            "do_augmentations": true
        }
    },
    "warper": {
        "type": "Warper",
        "args": {
        }
    },
    "optimizer": {
        "type": "Adam",
        "args": {
            "lr": 0.001,
            "weight_decay": 0,
            "amsgrad": true
        }
    },
    "loss": "dense_correlation_loss_dve",
    "metrics": [

    ],
    "visualizations": [
        "sphere_colormap"
    ],
    "lr_scheduler": {
        "type": "MultiStepLR",
        "args": {
            "milestones": [6, 8],
            "gamma": 0.1
        }
    },
    "trainer": {
        "epochs": 10,
        "save_dir": "saved/",
        "save_period": 1,
        "verbosity": 2,
        "monitor": "min val_loss",
        "early_stop": 10,
        "tensorboardX": true,
        "log_dir": "saved/"
    }
}
